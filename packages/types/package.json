{
  "name": "@enkryptcom/types",
  "version": "0.0.5",
  "description": "typescript types for enkrypt packages",
  "type": "module",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "publishConfig": {
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "types": "dist/index.d.ts"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --clean",
    "lint": "prettier --write .",
    "version:bump:patch": "yarn version --immediate patch",
    "publish": "yarn npm publish --access public",
    "test": "echo 'no-tests'"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^9.22.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-module-resolver": "^1.5.0",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "8.26.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/enkryptcom/enKrypt/tree/main/packages/types"
  },
  "keywords": [],
  "author": "kvhnuke",
  "license": "MIT"
}
